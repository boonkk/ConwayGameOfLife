<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="olech.marcin.view.FXMLController">
      <VBox id="VBox">
         <children>
             <HBox>
                 <children>
                     <Label text="Current starting pattern: "/>
                     <Label fx:id="currentPattern"/>
                 </children>
             </HBox>
            <Canvas fx:id="cellCanvas" height="125.0" width="290.0" />
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </VBox>
</AnchorPane>
